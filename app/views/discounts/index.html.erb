<%= render partial: "shared/header" %> 

<% @discounts.each do |discount| %>
<div id="<%=discount.id%>">
  <h3><%= link_to "Buy #{discount.quantity} get #{discount.percentage}% off", merchant_discount_path(discount.merchant_id, discount.id) %></h3>
  <%= link_to "Delete Discount", merchant_discount_path(@merchant.id, discount.id), method: :delete  %></br>
  </div>
<%end%>

<%= link_to "Create New Discount", new_merchant_discount_path(@merchant.id)%>